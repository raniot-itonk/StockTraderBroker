{
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Information"
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "Services": {
    "BankService": {
      "BankPath": {
        "Transfer": "/api/Accounts"
      }
    },
    "AuthorizationService": {
      "AuthPath": {
        "Register": "/api/Account/Register"
      }
    },
    "PublicShareOwnerControl": {
      "PublicSharePath": {
        "Stock": "/api/Stocks"
      }
    }
  }
}