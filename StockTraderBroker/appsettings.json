{
  "Logging": {
    "LogLevel": {
      "Default": "Warning",
      "System": "Warning",
      "Microsoft": "Warning"
    },
    "Console": {
      "IncludeScopes": true
    }
  },
  "Serilog": {
    "MinimumLevel": "Information",
    "Override": {
      "Microsoft": "Warning"
    },
    "Enrich": [ "FromLogContext", "WithMachineName" ]
  },
  "Services": {
    "BankService": {
      "BankPath": {
        "Transfer": "/api/Transfer",
        "Reservation": "/api/Reservation"
      }
    },
    "AuthorizationService": {
      "AuthPath": {
        "Register": "/api/Account/Register"
      }
    },
    "PublicShareOwnerControl": {
      "PublicSharePath": {
        "Stock": "/api/Stocks"
      }
    },
    "TobinTaxer": {
      "TobinTaxerPath": {
        "StockTax": "/api/StockTaxes"
      }
    }
  },
  "RabbitMqOptions": {
    "ExchangeName": "StockExchange",
    "RoutingKey": "History",
    "QueueName": "History",
    "VirtualHost": "/"
  }
}